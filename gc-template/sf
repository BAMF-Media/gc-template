}(document, "script", "vrlps-js", "8Jw_kEAzKoODX2CZXBJefEHH3pc", "waitlist");</script>
<script>
    $("#participation_form").submit(function (e) {
        e.preventDefault();
        participate(document.getElementById("signupEmail").value, document.getElementById("firstName").value);
    });

    function participate(signupEmail, firstName) {
        if (!signupEmail || !firstName) return;
        VL.options.form_fields.form_email = signupEmail;
        VL.options.form_fields.form_firstName = firstName;
        //submit the participant to Viral Loops
        VL.createLead(function () { //TODO change the url
            console.log("VL signup");
            location.href = "http://girlcrew.com/sf-refer";
        });
    }

    //if they are already registered, redirect them to the referral page
    window.addEventListener("vl_ready", function (e) {
        if (VL.refCode) { //TODO change the url
            location.href = "http://girlcrew.com/sf-refer";
        }
    }, false);

    $.get("https://app.viral-loops.com/api/v2/data?publicToken=8Jw_kEAzKoODX2CZXBJefEHH3pc&params%5Baccessors%5D=rank", function (data) {
        var initialNumber = 99; //TODO change the initial number
        var entries = initialNumber + data.rank;
        if (entries >= 2000) {
            progressBar(100);
        } else {
            progressBar(Math.floor((entries / 2000) * 100))
        }
        $("#currentWaitlist").text(entries + " of 2000 San Francisco Crew Members");
        $("#currentWaitlistPersent").text("( " + 100 * entries / 2000 + " % of goal )");

    });

    function progressBar(percent) {
        var bar = document.getElementById("progress");
        bar.style.width = percent + "%";
    }
</script>

